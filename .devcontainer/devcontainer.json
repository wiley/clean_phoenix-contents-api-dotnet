{
    "name": "contents-api",
    "dockerComposeFile": [
        "../dev/docker-compose.yml"
    ],
    "service": "contents-api",
    "workspaceFolder": "/app",
    "customizations":{
        "vscode": {
            "extensions": [
                "ms-dotnettools.csdevkit",
                "ms-dotnettools.csharp",
                "josefpihrt-vscode.roslynator",
